<Screen_Seventh>: #진행여부
    FloatLayout:
        canvas.before:
            Color:
                rgba: 1, 1, 1, 1
            Rectangle:
                pos: self.pos
                size: self.size
        Image:
            pos_hint: {'center_x': 0.5, 'center_y':0.6}
            source: 'images_en/hello_en.png'
            allow_stretch: True
            keep_ratio: True
            size_hint: None, None
            size: 1500, 1500
        BoxLayout:
            orientation: 'horizontal'
            padding: 50
            spacing: 100
            size_hint: 0.4, 0.4
            pos_hint: {'center_x': 0.5, 'center_y':0.25}
            canvas.before:
            Button:
                canvas.before:
                    Color: 
                        rgba: 0, 0, 0, 1  # 검정색 스트로크 색
                    RoundedRectangle:
                        pos: self.pos
                        size: self.size
                        radius: [20]  # 둥근 정도 (숫자 높을수록 둥글게)
                text: "Yes"
                font_name: 'fonts/Pretendard/public/static/alternative/Pretendard-Bold.ttf'
                font_size: 50
                size_hint: (0.15, 0.3)
                background_normal: ''
                background_down: ''
                background_color: 1, 1, 1, 1
                color: 0, 0, 0, 1
                on_release: root.manager.current = 'eighth'
                canvas.after:
                    Color:
                        rgba: 0, 0, 0, 1  # 테두리 색 (검정)
                    Line:
                        width: 2
                        rounded_rectangle: (self.x, self.y, self.width, self.height, 20)  # 마지막 숫자가 둥글기
            Button:
                canvas.before:
                    Color: 
                        rgba: 0, 0, 0, 1  # 검정색 스트로크 색
                    RoundedRectangle:
                        pos: self.pos
                        size: self.size
                        radius: [20]  # 둥근 정도 (숫자 높을수록 둥글게)
                text: "No"
                font_name: 'fonts/Pretendard/public/static/alternative/Pretendard-Bold.ttf'
                font_size: 50
                size_hint: (0.15, 0.3)
                background_normal: ''
                background_down: ''
                background_color: 1, 1, 1, 1
                color: 0, 0, 0, 1
                on_release: root.manager.current = 'zero'
                canvas.after:
                    Color:
                        rgba: 0, 0, 0, 1  # 테두리 색 (검정)
                    Line:
                        width: 2
                        rounded_rectangle: (self.x, self.y, self.width, self.height, 20)  # 마지막 숫자가 둥글기

