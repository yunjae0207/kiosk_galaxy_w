        font_name: 'fonts/Pretendard/public/static/alternative/Pretendard-Bold.ttf'
        font_size: 55
        color: 0, 0, 0, 1
    Button:
        background_normal: 'images_en/out_en.png'
        background_down: 'images_en/out_en.png'
        size_hint_x: 0.1
        on_press: root.on_delete(root.item_text)

<Button_Sub_En>: #3번째 스크린 서브 화면 버튼 
    canvas.before:
        Color:
            rgba: 0, 0, 0, 1  # 검정색 스트로크 색
        RoundedRectangle:
            pos: self.pos
            size: self.size
            radius: [20]  # 둥근 정도 (숫자 높을수록 둥글게)
    
    text: f"{self.product_name} - {self.price}"
    font_name: 'fonts/Pretendard/public/static/alternative/Pretendard-Bold.ttf'
    font_size: 40
    background_normal: ''
    background_down: ''
    background_color: 1, 1, 1, 1
    color: 0, 0, 0, 0
    size_hint: None, None
    size: 800, 800
    on_release: app.show_popup_store_en(self.text)
    
    canvas.after:
        Color:
            rgba: 0, 0, 0, 1  # 테두리 색 (검정)
        Line:
            width: 2
            rounded_rectangle: (self.x, self.y, self.width, self.height, 20)  # 마지막 숫자가 둥글기

<Button_Pay_En>: #결제 선택지 버튼
