ScreenManager: #전체 화면 전환용 스크린 매니저
    id: main_manager
    Screen_Zero:
        name: 'zero' 
    Screen_Language:
        name: 'language'
    Screen_First:
        name: 'first'   
    Screen_Second:
        name: 'second'
    Screen_Third:
        name: 'third'
    Screen_Fourth:
        name: 'fourth'
    Screen_Fifth:
        name: 'fifth'
    Screen_Sixth:
        name: 'sixth'
    Screen_Seventh:
        name: 'seventh'
    Screen_Eighth:
        name: 'eighth'
    Screen_Ninth:
        name: 'ninth'
    Screen_Tenth:
        name: 'tenth'
    Screen_Eleventh:
        name: 'eleventh'
    Screen_Twelfth:
        name: 'twelfth'

<Screen_Zero>:
    FloatLayout:
        canvas.before:
            Color:
                rgba: 1, 1, 1, 1
            Rectangle:
                pos: self.pos
                size: self.size
        Label:
            text: "[color=F69D6A]CAUTogether[/color]  [color=08BD77]기획봉사단 7기[/color]"
            markup: True
            font_name: 'fonts/Pretendard/public/static/alternative/Pretendard-Bold.ttf'
            font_size: 70
            pos_hint: {'center_x': 0.5, 'center_y': 0.9}
            color: 0, 0, 0, 1
        Button:
            pos_hint: {'center_x': 0.5, 'center_y': 0.45}
            size_hint: None, None
            size: 1500, 1500  # width=200, height=100
            background_normal: 'images/openning.png'
            background_down: 'images/openning.png'
            color: 0, 0, 0, 1
            on_release: root.manager.current = 'language'

<Screen_Language>:
    FloatLayout:
        canvas.before:
            Color:
                rgba: 1, 1, 1, 1
            Rectangle:
                pos: self.pos
                size: self.size
        BoxLayout:
            pos_hint: {'center_x': 0.5, 'center_y':0.7}
            spacing: 30
            padding: 50
            canvas.before:
            Button:
                canvas.before:
                    Color: 
                        rgba: 0, 0, 0, 1  # 검정색 스트로크 색
                    RoundedRectangle:
                        pos: self.pos
                        size: self.size
                        radius: [20]  # 둥근 정도 (숫자 높을수록 둥글게)
                text: "한글"
                font_name: 'fonts/Pretendard/public/static/alternative/Pretendard-Bold.ttf'
                font_size: 50
                size_hint: (0.4, 0.5)
                background_normal: ''
                background_down: ''
                background_color: 1, 1, 1, 1
                color: 0, 0, 0, 1
                on_release: root.manager.current = 'first'
                canvas.after:
                    Color:
                        rgba: 0, 0, 0, 1  # 테두리 색 (검정)
                    Line:
                        width: 2
                        rounded_rectangle: (self.x, self.y, self.width, self.height, 20)  # 마지막 숫자가 둥글
            Button:
                canvas.before:
                    Color: 
                        rgba: 0, 0, 0, 1  # 검정색 스트로크 색
                    RoundedRectangle:
                        pos: self.pos
                        size: self.size
                        radius: [20]  # 둥근 정도 (숫자 높을수록 둥글게)
                text: "English"
                font_name: 'fonts/Pretendard/public/static/alternative/Pretendard-Bold.ttf'
                font_size: 50
                size_hint: (0.4, 0.5)
                background_normal: ''
                background_down: ''
                background_color: 1, 1, 1, 1
                color: 0, 0, 0, 1
                on_release: root.manager.current = 'seventh'
                canvas.after:
                    Color:
                        rgba: 0, 0, 0, 1  # 테두리 색 (검정)
                    Line:
                        width: 2
                        rounded_rectangle: (self.x, self.y, self.width, self.height, 20)  # 마지막 숫자가 둥글기

